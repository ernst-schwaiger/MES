{                                                                                                             
    // Use IntelliSense to learn about possible attributes.                                                   
    // Hover to view descriptions of existing attributes.                                                                 
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    // For debugging a certain fuzz test, change the name of the test folder, ensure the debug binary
    // is built, and the CORPUS folder as stated in args exists.
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Firmware Manager Daemon Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/FWManager/FWManager",
            "args": [ "/home/kali/FWManager"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Signing Tool Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/BuildSystem/signing-tool",
            "args": [ "123456",  
                      "${workspaceFolder}/BuildSystem/signedFile.txt", 
                      "${workspaceFolder}/BuildSystem/signedFile.txt.sig"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}